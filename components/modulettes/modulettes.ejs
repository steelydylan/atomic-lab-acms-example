<!-- BEGIN_SetTemplate id="modulettes" -->
<div class="modulettes">
    <a class="modulette default" href="{{url}}">
      <img class="icon callout-icon" src="/static/images/callout_default.svg" alt="">
      <div class="info">
        <div class="title">{{title}}</div>
        <span class="subtitle">{{subtitle}}</span>
      </div>
    </a>
</div>
<!-- END_SetTemplate -->

